function getZipLength(){var zip_length=5;if(window.sitecode=='olxro'||window.sitecode=='olxby'||window.sitecode=='olxkz'){zip_length=6}else if(window.sitecode=='olxua'){zip_length=5}else if(window.sitecode=='olxpt')zip_length=4;return zip_length}
function initKeyUp(settings){var $input=settings.input,$fakeBackground=$('#cityFieldGray');if(settings.citiesOld){$input.keydown(function(e){e.preventDefault();return false})}else $input.keyup(function(e){if(e.keyCode==40||e.keyCode==13||e.keyCode==9||e.keyCode==38||e.keyCode==37||e.keyCode==39)return;var val=$.trim($input.val());val?settings.clearTextButton.removeClass('hidden'):settings.clearTextButton.addClass('hidden');$('.vishid',$fakeBackground).text(val);if(!val||val.length<2||(/^\d/.test(val)&&val.length<getZipLength())&&!/^\d{1,2} \w+$/.test(val)){settings.suggestUl.text('').addClass('hidden');settings.input.removeClass('hidden-focus');if(settings.bigLayer){val?$(settings.bigLayer).hide():$(settings.bigLayer).show();$('#back_region_link').click()};return};if(/^\d{5}/.test(val)&&window.sitecode=='olxpl')$input.val(val.substring(0,2)+'-'+val.substring(2));$input.data('lastChanged',new Date().getTime());settings.ajaxCounter++;setTimeout(function(){var diff=new Date().getTime()-parseInt($input.data('lastChanged'));if(diff<(settings.keyUpSpeed-50)){settings.ajaxCounter--;settings.ajaxCounter=Math.max(0,settings.ajaxCounter);return};var currentVal=$.trim($input.val());$.post(settings.url,{data:currentVal,withRegions:settings.showRegions?1:0,skipDistricts:settings.skipDistricts?1:0},function(data){settings.ajaxCounter--;settings.ajaxCounter=Math.max(0,settings.ajaxCounter);$input.data('query',currentVal);settings.suggestUl.text('').addClass('hidden');settings.input.removeClass('hidden-focus');if(settings.bigLayer)$(settings.bigLayer).hide();if(data.status!='ok'){settings.onEmptyResult();return};hideError(settings);var pos=1;for(i in data.data){var row=data.data[i],text=row.text,id=row.id,url=row.url,districts=data.data[i]['districts']?data.data[i]['districts']:[],isRegion=data.data[i]['region']||data.data[i]['isRegion'],tokens=currentVal.split(/[.,\/ -]+/),option=text,options=option.split(/\s/);for(t in tokens)for(o in options)if(options[o].search('<'+settings.mar