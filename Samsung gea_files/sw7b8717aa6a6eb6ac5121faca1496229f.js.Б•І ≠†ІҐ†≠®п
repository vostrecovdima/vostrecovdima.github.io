function saveAsDefaultLocationInCookie(regionId,cityID,districtID,dist,settings){$.post(www_base_ajax+'/geo6/savedefaultlocationmainpage/',{region_id:regionId,city_id:cityID,district_id:districtID,dist:dist,addToLast:settings.saveAsLastLocationInCookie?1:0},function(data){settings.cookiesLocationSaved&&settings.cookiesLocationSaved()})}
function loadDefaultLocationFromCookie(settings){if(!$.cookie('my_city_2')||!settings.input.val())return;var value=$.cookie('my_city_2').split('_');if(value.length>=4){var isRegion=parseInt(value[4])?1:0,grayText=value[5]==value[3]?'':value[5];setFormInputs(settings,value[3].replace(/\++/g,' '),'',isRegion?value[0]:'',isRegion?'':value[0],value[1],value[2],false,grayText,value[6]);settings.defaultLocationFromCookieLoaded&&settings.defaultLocationFromCookieLoaded(value)}}
function getURLDistance(){var val=parseInt(unescape(location.search).split('search[dist]=')[1]);return val?val:null}
function getDefaultDistance(){return typeof window.defaultSearchDistance!=='undefined'?window.defaultSearchDistance:5}
function showGeoSuggestions(settings){if($.cookie('disabledgeo')==null&&window.city_id==''&&window.region_id==''){if(typeof geoData!=='undefined'&&geoData.search&&geoData.search['user_id'])return;var $geoSuggestions=$('#geo-suggestions');$.get(www_base_ajax+'/geo6/get/',function(data){if(data.length){$('#geo-suggestions-options').html('');for(c in data){$geoSuggestions.removeClass('hidden');$('#geo-suggestions-options').append('<a data-type="'+data[c]['type']+'" data-label="'+data[c]['label']+'" data-name="'+data[c]['name']+'" data-id="'+data[c]['id']+'" href="#" class="inlblk tdnone br3">'+data[c]['name']+'</a>').on('click','#geo-suggestions-options a',function(){var $this=$(this);settings.input.data('cityLabel',$this.data('label'));if($this.data('type')=='region'){setFormInputs(settings,$this.data('label'),'',$this.data('id'),'','',0,true,'',$this.data('url'))}else if($this.data('type')=='other'){if(typeof otherFirst=='undefined'){otherFirst=true;$('#cityField').click().focus().addClass('ca2')}}else setFormInputs(settings,$this.data('label'),'','',$this.data('id'),0,getDefaultDistance(),true,$this.data('graytext'),$this.data('url'));$.cookie('disabledgeo',1,{expires:30,path:'/'});$geoSuggestions.remove();return false});$geoSuggestions.css('marginLeft',-(parseInt($geoSuggestions.width()/2,10)+13))}}});$('#geo-suggestions-close').click(function(){$.cookie('disabledgeo',1,{expires:30,path:'/'});$geoSuggestions.remove();settings.input.removeClass('hidden-focus');return false})}}
