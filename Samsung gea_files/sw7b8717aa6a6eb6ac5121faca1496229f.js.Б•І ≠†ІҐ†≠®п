function getStorageData(key){if(testLocalStore()){return getFromLocalStorage(key)}else return getCookie(key)}
function setStorageData(key,value,minutes){if(testLocalStore()){setInLocalStorage(key,value,minutes)}else setCookie(key,value,minutes)}
function setInLocalStorage(name,value,minutes){var d=new Date();d.setTime(d.getTime()+minutes*60*1e3);window.localStorage.setItem(name,JSON.stringify({value:value,expires:d.getTime()}))}
function getFromLocalStorage(name){var value=window.localStorage.getItem(name);if(value!==null){value=JSON.parse(value);if(typeof value.expires!=="undefined"){var d=new Date();if(value.expires>d.getTime()){value=value.value}else return null}};return value}
function setCookie(name,value,minutes){var d=new Date();d.setTime(d.getTime()+minutes*60*1e3);var expires="expires="+d.toUTCString();document.cookie=name+"="+value+";"+expires+";path=/"}
function getCookie(name){var match=document.cookie.match(new RegExp('(^| )'+name+'=([^;]+)'));if(match)return match[2];return null}
function testLocalStore(){if(window.hasLocalStore!==undefined)return window.hasLocalStore;var testmod='test';try{localStorage.setItem(testmod,testmod);localStorage.removeItem(testmod);window.hasLocalStore=true;return true}catch(e){window.hasLocalStore=false;return false}};if(window.amanager!==undefined&&window.amanager){initAManager();if(advertsAreEnabled){if(typeof window.performanceData!=="undefined"){advertPerformance(window.performanceData);if(typeof window.aPerformance!=="undefined"){window.aPerformance.initData();window.aPerformance.registerPv()}};if(typeof window.cpmScalingData!=='undefined'){cpmScaling(window.controlEngineSetting,window.cpmScalingData);if(typeof window.aCpmScaling!=='undefined'){window.aCpmScaling.initData();GPT.targeting=window.aCpmScaling.changePriceFloor(GPT.targeting,'adx')}};window.initDfpSlots();window.displaySlots()}};(function(){var DFPClass=Class.extend({init:function(options){for(var name in options)this[name]=options[name];var thiz=this;$(window).bind('ajaxReady',function(){thiz.refreshSlot()});$(window).bind('startReloadContent',function(){thiz.sendEventsToNinja();if(typeof window.aPerformance!=="undefined")window.aPerformance.saveData();if(typeof window.aCpmScaling!=='undefined'&&window.slotsOnPage!=='undefined'){window.aCpmScaling.calculateTheGroupAverage(window.slotsOnPage);window.aCpmScaling.registerData()}});thiz.refreshSlot();thiz.initLazyLoadSlot()},sendEventsToNinja:function(){if(typeof trackerFactoryInstance!=="undefined"&&typeof NinjaTracker!=="undefined"){NinjaTracker.push(NinjaTracker.mergeValidOptionalParams(NinjaTracker.getDataFrom(trackingData.pageView),trackerFactoryInstance.getRenderEvents()));NinjaTracker.push(NinjaTracker.mergeValidOptionalParams(NinjaTracker.getDataFrom(trackingData.pageView),trackerFactoryInstance.getShowEvents()));trackerFactoryInstance.clearRenderEvents();trackerFactoryInstance.clearShowEvents();trackerFactoryInstance.clearSavedInfoAboutClick()}},clearOldInterval:function(slotId){if(this.ch